<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="38dp"
    android:paddingLeft="4dp"
    android:orientation="horizontal"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <TextView
        style="@style/report_measure_item_text_style"
        android:id="@+id/gen_exam_title"
        android:layout_width="0dp"
        android:gravity="center_vertical"
        android:layout_height="match_parent"
        android:layout_weight="4.7"/>

    <!--<TextView
        style="@style/report_measure_item_text_style"
        android:id="@+id/gen_exam_ga"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.5"/>-->

    <!--<TextView
        android:id="@+id/gen_exam_edd"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:gravity="center"
        android:textColor="@color/white"/>-->

    <TextView
        style="@style/report_measure_item_text_style"
        android:id="@+id/gen_exam_value"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="3"
        android:gravity="center"/>

    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_measure_value_edit_style"
        android:id="@+id/gen_exam_v1"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:enabled="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:digits=".0987654321"
        android:layout_margin="1dp"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_measure_value_edit_style"
        android:id="@+id/gen_exam_v2"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:enabled="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:digits=".0987654321"
        android:layout_margin="1dp"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_measure_value_edit_style"
        android:id="@+id/gen_exam_v3"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:enabled="false"
        android:focusable="false"
        android:digits=".0987654321"
        android:focusableInTouchMode="false"
        android:layout_margin="1dp"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_measure_value_edit_style"
        android:id="@+id/gen_exam_v4"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:enabled="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:layout_margin="1dp"
        android:digits=".0987654321"/>

    <edan.am57.bx4.common.widget.CustomSpinner
        style="@style/report_spinner_style"
        android:id="@+id/gen_exam_method"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.8"
        android:enabled="false"
        android:layout_margin="1dp"/>


    <TextView
        style="@style/report_measure_item_text_style"
        android:id="@+id/gen_exam_cua"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="3.5"
        android:layout_margin="1dp"
        android:gravity="center"/>

</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="38dp"
    android:orientation="horizontal"
    android:paddingLeft="4dp"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <TextView
        style="@style/report_measure_item_text_style"
        android:id="@+id/ob_fetus2d_title"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:layout_weight="2.5"/>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center">

        <CheckBox
            style="@style/main_screen_checkbox_style"
            android:id="@+id/c_is_cua_or_aua"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:gravity="center"
            android:checked="true"/>
    </RelativeLayout>

    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/ob_fetus2d_value"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:paddingRight="2dp"
        android:layout_weight="2.5"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_measure_value_edit_style"
        android:id="@+id/ob_fetus2d_v1"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:enabled="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:layout_margin="1dp"
        android:digits=".0987654321"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_measure_value_edit_style"
        android:id="@+id/ob_fetus2d_v2"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:enabled="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:digits=".0987654321"
        android:layout_margin="1dp"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_measure_value_edit_style"
        android:id="@+id/ob_fetus2d_v3"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:enabled="false"
        android:focusable="false"
        android:digits=".0987654321"
        android:focusableInTouchMode="false"
        android:layout_margin="1dp"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_measure_value_edit_style"
        android:id="@+id/ob_fetus2d_v4"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:enabled="false"
        android:focusable="false"
        android:digits=".0987654321"
        android:focusableInTouchMode="false"
        android:layout_margin="1dp"/>

    <edan.am57.bx4.common.widget.CustomSpinner
        style="@style/report_spinner_style"
        android:id="@+id/ob_fetus2d_method"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.8"
        android:enabled="false"
        android:layout_margin="1dp"/>

    <edan.am57.bx4.common.widget.CustomSpinner
        style="@style/report_spinner_style"
        android:id="@+id/ob_fetus2d_reference"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2.5"
        android:enabled="false"
        android:layout_margin="1dp"/>



    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/ob_fetus2d_ga"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_weight="1.3"/>

    <!--计划中会使用进度条 预留-->
    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/ob_fetus2d_edd"
        android:layout_width="0dp"
        android:gravity="center"
        android:layout_height="match_parent"
        android:layout_weight="2.5"
        android:visibility="invisible"/>


</LinearLayout>
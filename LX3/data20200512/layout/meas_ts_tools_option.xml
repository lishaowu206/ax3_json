<?xml version="1.0" encoding="utf-8"?>
<!--<RelativeLayout-->
<edan.am57.bx4.measurement.Layout.MeasToolAndResultLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/touch_screen_tab_content"
    android:paddingBottom="@dimen/measure_ts_tool_option_padding_bottom"
    android:paddingTop="@dimen/measure_ts_tool_option_padding_top">

    <edan.am57.bx4.measurement.Layout.MeasToolGridLaout
        android:id="@+id/meas_tool_gridlayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/touch_screen_button_height"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="@dimen/measure_ts_tool_option_items_margin_right"
        android:layout_marginStart="@dimen/measure_ts_tool_option_items_margin_left"
        android:columnCount="5"
        android:rowCount="1">
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="3"
            android:layout_columnWeight="1"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="4"
            android:layout_columnWeight="1"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>
    </edan.am57.bx4.measurement.Layout.MeasToolGridLaout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/touch_screen_tab_content_sep"
        android:layout_below="@+id/meas_tool_gridlayout"
        android:layout_marginEnd="@dimen/measure_ts_tool_option_margin_right"
        android:layout_marginTop="@dimen/measure_ts_tool_option_half_spacing_vertical"
        android:layout_marginStart="@dimen/measure_ts_tool_option_margin_right"
        android:background="@color/touch_screen_tab_content_sep"/>

    <edan.am57.bx4.measurement.Layout.MeasResultGridLayout
        android:id="@+id/meas_result_gridlayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/meas_tool_gridlayout"
        android:layout_marginEnd="@dimen/measure_ts_tool_option_items_margin_right"
        android:layout_marginStart="@dimen/measure_ts_tool_option_items_margin_left"
        android:layout_marginTop="@dimen/measure_ts_tool_option_spacing_vertical"
        android:columnCount="5"
        android:rowCount="3">
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="3"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="4"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"/>

        <!--Row 2-->
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="1"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="1"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="1"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="3"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="1"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="4"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="1"
            android:layout_rowWeight="1"/>

        <!--Row 3-->
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="0"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="2"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="1"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="2"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="2"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="2"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="3"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="2"
            android:layout_rowWeight="1"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_column="4"
            android:layout_columnWeight="1"
            android:layout_marginBottom="@dimen/measure_ts_tool_option_spacing_vertical"
            android:layout_marginEnd="@dimen/measure_ts_tool_option_spacing_horizontal"
            android:layout_row="2"
            android:layout_rowWeight="1"/>
    </edan.am57.bx4.measurement.Layout.MeasResultGridLayout>
    <!--</RelativeLayout>-->
</edan.am57.bx4.measurement.Layout.MeasToolAndResultLayout>
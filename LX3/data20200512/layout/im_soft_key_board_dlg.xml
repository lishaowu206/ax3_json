<?xml version="1.0" encoding="utf-8"?>
<edan.asp.inputmethod.softkb.SoftKeyboardLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:userdefine="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:paddingLeft="2dp"
	android:paddingRight="2dp"
	android:background="@color/soft_kb_bg"
	userdefine:skb_preview_enable="false"
	userdefine:skb_width="@dimen/skb_keyboard_width"
	userdefine:skb_preview_height="@dimen/skb_preview_height"
	userdefine:skb_candidate_height="@dimen/skb_candidate_height"
	userdefine:skb_keyboard_height="@dimen/skb_keyboard_height"
	userdefine:skb_backcolor="@color/soft_keyboard_backcolor"
	userdefine:input_preview_backcolor="@color/soft_keyboard_backcolor"
	userdefine:input_preview_textcolor="@color/soft_kb_text"
	userdefine:input_preview_textsize="@dimen/skb_preview_text_size"

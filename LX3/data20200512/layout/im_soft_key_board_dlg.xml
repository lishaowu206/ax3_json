	userdefine:cand_preview_textsize="@dimen/skb_candidate_preview_text_size"
	userdefine:cand_candword_btn_textcolor="@color/soft_kb_text"
	userdefine:cand_candword_btn_textsize="@dimen/skb_candidate_btn_text_size"
	userdefine:cand_candword_btn_background="@drawable/key_bg"
	userdefine:cand_page_btn_textsize="@dimen/skb_candidate_page_btn_text_size"
	userdefine:cand_page_btn_width="@dimen/skb_candidate_page_btn_width"
	userdefine:kb_keyBackground="@drawable/key_bg"
	userdefine:kb_labelTextSize="@dimen/skb_key_common_text_size"
	userdefine:kb_keyTextSize="@dimen/skb_key_function_text_size"
	userdefine:kb_keyTextColor="@color/soft_kb_text"
/>


        android:layout_toRightOf="@id/report_worksheet_OB_EDDCUA_tv"
        android:layout_marginTop="5dp"/>

    <TextView
        style="@style/report_top_info_text_style"
        android:id="@+id/report_worksheet_OB_fetuspos_tv"
        android:layout_below="@id/report_worksheet_OB_GACUA_tv"
        android:layout_alignEnd="@id/report_worksheet_OB_LMP_tv"
        android:layout_marginTop="5dp"
        android:paddingLeft="30dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:text="@string/report_worksheet_ob_fetuspos"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_top_info_edit_style"
        android:id="@+id/report_worksheet_OB_fetuspos_edit"
        android:layout_below="@id/report_worksheet_OB_GACUA_tv"
        android:layout_toRightOf="@id/report_worksheet_OB_fetuspos_tv"
        android:maxLength="64"
        android:singleLine="true"
        android:layout_marginTop="5dp"
        android:enabled="true"
        android:focusable="false"
        android:focusableInTouchMode="false"/>

    <TextView
        style="@style/report_top_info_text_style"
        android:id="@+id/report_worksheet_OB_plac_tv"
        android:layout_below="@id/report_worksheet_OB_GACUA_tv"

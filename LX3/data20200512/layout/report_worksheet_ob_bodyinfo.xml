    <TextView
        style="@style/report_top_info_text_style"
        android:id="@+id/report_worksheet_OB_GACUA_tv"
        android:layout_below="@id/report_worksheet_OB_LMP_tv"
        android:layout_alignEnd="@id/report_worksheet_OB_ClinicalGA_tv"
        android:text="@string/report_worksheet_ob_gacua"
        android:layout_marginTop="5dp"/>

    <EditText
        style="@style/report_top_info_edit_style"
        android:id="@+id/report_worksheet_OB_GACUA_edit"
        android:layout_below="@id/report_worksheet_OB_LMP_tv"
        android:layout_toRightOf="@id/report_worksheet_OB_GACUA_tv"
        android:layout_marginTop="5dp"/>

    <TextView
        style="@style/report_top_info_text_style"
        android:id="@+id/report_worksheet_OB_EDDCUA_tv"
        android:layout_below="@id/report_worksheet_OB_LMP_tv"
        android:layout_toRightOf="@id/report_worksheet_OB_GACUA_edit"
        android:layout_alignStart="@id/report_worksheet_OB_ClinicalEDD_tv"
        android:layout_marginTop="5dp"
        android:text="@string/report_worksheet_ob_eddcua"/>

    <EditText
        style="@style/report_top_info_edit_style"
        android:id="@+id/report_worksheet_OB_EDDCUA_edit"

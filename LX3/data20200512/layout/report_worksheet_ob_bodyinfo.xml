<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="155dp">

    <TextView
        style="@style/report_top_info_text_style"
        android:id="@+id/report_worksheet_OB_LMP_tv"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:paddingLeft="30dp"
        android:text="@string/report_worksheet_ob_lmp"/>

    <EditText
        style="@style/report_top_info_edit_style"
        android:id="@+id/report_worksheet_OB_LMP_edit"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/report_worksheet_OB_LMP_tv"/>


    <TextView
        style="@style/report_top_info_text_style"
        android:id="@+id/report_worksheet_OB_ClinicalGA_tv"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/report_worksheet_OB_LMP_edit"
        android:layout_marginLeft="50dp"
        android:text="@string/report_worksheet_ob_clinicalga"/>

    <EditText
        style="@style/report_top_info_edit_style"
        android:id="@+id/report_worksheet_OB_ClinicalGA_edit"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/report_worksheet_OB_ClinicalGA_tv"/>

    <TextView
        style="@style/report_top_info_text_style"
        android:id="@+id/report_worksheet_OB_ClinicalEDD_tv"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/report_worksheet_OB_ClinicalGA_edit"
        android:layout_marginLeft="50dp"
        android:text="@string/report_worksheet_ob_clinicaledd"/>

    <EditText
        style="@style/report_top_info_edit_style"
        android:id="@+id/report_worksheet_OB_ClinicalEDD_edit"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/report_worksheet_OB_ClinicalEDD_tv"/>


        android:layout_marginTop="5dp"
        android:text="@string/report_worksheet_ob_plac"/>

    <edan.am57.bx4.report.widget.CustomValueVaryEditText
        style="@style/report_top_info_edit_style"
        android:id="@+id/report_worksheet_OB_plac_edit"
        android:layout_below="@id/report_worksheet_OB_GACUA_tv"
        android:layout_toRightOf="@id/report_worksheet_OB_plac_tv"
        android:layout_marginTop="5dp"
        android:maxLength="64"
        android:singleLine="true"
        android:enabled="true"
        android:focusable="false"
        android:focusableInTouchMode="false"/>

    <include layout="@layout/report_worksheet_devider"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginTop="9dp"
        android:layout_below="@+id/report_worksheet_OB_fetuspos_tv"/>

</merge>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:orientation="horizontal">

    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/fetus_2d_title"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2.4"/>

    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/ob_cua_aua_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="" />

    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2.5"
        android:text="@string/report_worksheet_head_value" />

    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:text="@string/report_worksheet_head_value_1" />

    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:text="@string/report_worksheet_head_value_2" />

    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:text="@string/report_worksheet_head_value_3" />

    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.95"
        android:text="@string/report_worksheet_head_value_4" />

    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.8"
        android:text="@string/report_worksheet_head_method" />

    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/ob_reference_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2.5"
        android:text="@string/report_worksheet_ob_reference" />

    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.3"
        android:text="@string/report_worksheet_ob_ga" />

    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2.5"
        android:text="GP"
        android:visibility="invisible"/>

</LinearLayout>
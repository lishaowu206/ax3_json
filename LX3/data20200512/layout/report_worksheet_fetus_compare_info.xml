<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="38dp"
    android:layout_marginLeft="16dp"
    android:orientation="horizontal">

    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/fetus_compare_title_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="3"
        android:layout_margin="2dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="2dp"
        android:gravity="left|center_vertical"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="2dp"/>

    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/fetus_compare_a_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="@drawable/worksheet_edit_bg"
        android:layout_weight="2"
        android:layout_margin="2dp"/>

    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/fetus_compare_b_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="@drawable/worksheet_edit_bg"
        android:layout_weight="2"
        android:layout_margin="2dp"/>

    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/fetus_compare_c_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="@drawable/worksheet_edit_bg"
        android:layout_weight="2"
        android:visibility="invisible"
        android:layout_margin="2dp"/>

    <TextView
        style="@style/report_measure_title_text_style"
        android:id="@+id/fetus_compare_d_tv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="@drawable/worksheet_edit_bg"
        android:layout_weight="2"
        android:visibility="invisible"
        android:layout_margin="2dp"/>

    <!--占位-->
    <TextView
        style="@style/report_measure_title_text_style"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="5"/>

</LinearLayout>